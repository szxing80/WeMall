<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping >
	<!--订单明细信息-->
	<class entity-name="order_form" table="order_form" dynamic-update="true">
	    <id name="id" type="string">
				<generator class="uuid.hex"></generator>
		</id>
        <!-- 订单ID -->
        <property name="order_form_ID" type="string" />
        <!-- 时间 -->
        <property name="Date" type="string" />
        <!-- 会员ID -->
        <property name="vip_ID" type="string" />
        <!-- 收货地址 -->
        <property name="shopping_Address" type="string" />
        <!-- 商品总成本价 -->
        <property name="commodity_Total_Cost" type="string" />
        <!-- 商品总销售价 -->
        <property name="commodity_Total_Price" type="string" />
    </class>
</hibernate-mapping>